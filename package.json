{
  "name": "postal-code-jp",
  "version": "0.2.4",
  "description": "Address search from postal code for Japanese",
  "main": "lib/index.js",
  "files": [
    "lib/**/*.js",
    "example/**/*.js",
    "json/**/*.json",
    "README.md"
  ],
  "scripts": {
    "test": "multi='dot=- mocha-istanbul=-' ISTANBUL_REPORTERS='text-summary,lcovonly' ./node_modules/.bin/mocha test",
    "instrument": "node_modules/.bin/istanbul instrument lib -o lib-cov",
    "build": "./node_modules/.bin/babel -d lib -s inline src",
    "watch": "./node_modules/.bin/babel -d lib -s inline -w src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holyshared/postal-code-jp.git"
  },
  "author": "Noritaka Horio <holy.shared.design@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/holyshared/postal-code-jp/issues"
  },
  "homepage": "https://github.com/holyshared/postal-code-jp#readme",
  "dependencies": {
    "bluebird": "^3.0.6",
    "es6-error": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-preset-es2015": "^6.3.13",
    "codecov": "^1.0.1",
    "coffee-script": "^1.10.0",
    "eslint": "^2.2.0",
    "espower-coffee": "^1.0.1",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.3",
    "mocha-istanbul": "^0.2.0",
    "mocha-multi": "^0.9.0",
    "power-assert": "^1.1.0"
  }
}
